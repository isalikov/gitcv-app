{
    "name": "gitcv-app",
    "version": "0.0.1",
    "description": "Git CV Web App",
    "author": {
        "name": "Iakov Salikov",
        "email": "salikovpro@gmail.com"
    },
    "scripts": {
        "dist": "webpack -c .webpack/webpack.config",
        "dev": "webpack serve -c .webpack/webpack.config",
        "prepare": "husky install"
    },
    "eslintIgnore": [
        "dist",
        "node_modules",
        "src/assets"
    ],
    "eslintConfig": {
        "plugins": [
            "prettier"
        ],
        "extends": [
            "airbnb",
            "airbnb-typescript",
            "plugin:prettier/recommended"
        ],
        "parserOptions": {
            "project": "./tsconfig.json"
        },
        "rules": {
            "import/prefer-default-export": [
                "off"
            ],
            "import/extensions": [
                "off"
            ],
            "import/order": [
                "error",
                {
                    "groups": [
                        "external",
                        [
                            "builtin",
                            "internal",
                            "unknown",
                            "parent",
                            "sibling",
                            "index",
                            "object",
                            "type"
                        ]
                    ],
                    "pathGroupsExcludedImportTypes": [],
                    "pathGroups": [
                        {
                            "pattern": "react",
                            "group": "external",
                            "position": "before"
                        },
                        {
                            "pattern": "@gitcv/**",
                            "group": "external",
                            "position": "after"
                        },
                        {
                            "pattern": "@app/**",
                            "group": "external",
                            "position": "after"
                        },
                        {
                            "pattern": "@lib/**",
                            "group": "external",
                            "position": "after"
                        }
                    ],
                    "alphabetize": {
                        "order": "asc"
                    },
                    "newlines-between": "always-and-inside-groups"
                }
            ],
            "react/require-default-props": [
                "off"
            ],
            "react/react-in-jsx-scope": [
                "off"
            ],
            "react/prop-types": [
                "off"
            ],
            "react/function-component-definition": [
                "off"
            ],
            "@typescript-eslint/naming-convention": [
                "off"
            ]
        }
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "printWidth": 80
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env"
            ],
            [
                "@babel/preset-react",
                {
                    "runtime": "automatic"
                }
            ],
            [
                "@babel/preset-typescript"
            ]
        ]
    },
    "dependencies": {
        "@babel/core": "^7.22.1",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.22.4",
        "@babel/preset-react": "^7.22.3",
        "@babel/preset-typescript": "^7.21.5",
        "@editorjs/checklist": "^1.5.0",
        "@editorjs/code": "^2.8.0",
        "@editorjs/delimiter": "^1.3.0",
        "@editorjs/editorjs": "^2.27.0",
        "@editorjs/embed": "^2.5.3",
        "@editorjs/header": "^2.7.0",
        "@editorjs/image": "^2.8.1",
        "@editorjs/inline-code": "^1.4.0",
        "@editorjs/link": "^2.5.0",
        "@editorjs/list": "^1.8.0",
        "@editorjs/marker": "^1.3.0",
        "@editorjs/paragraph": "^2.9.0",
        "@editorjs/quote": "^2.5.0",
        "@editorjs/raw": "^2.4.0",
        "@editorjs/simple-image": "^1.5.1",
        "@editorjs/table": "^2.2.1",
        "@editorjs/warning": "^1.3.0",
        "@isalikov/gitcv-api": "^0.0.1",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
        "@reduxjs/toolkit": "^1.9.5",
        "@rsuite/icons": "^1.0.2",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@webpack-cli/serve": "^2.0.4",
        "autoprefixer": "^10.4.14",
        "axios": "^1.4.0",
        "babel-loader": "^9.1.2",
        "classnames": "^2.3.2",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "eslint": "^8.41.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "html-webpack-plugin": "^5.5.1",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.2",
        "prettier": "2.8.8",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-editor-js": "^2.1.0",
        "react-intl": "^6.4.2",
        "react-redux": "^8.0.7",
        "react-refresh": "^0.14.0",
        "react-router-dom": "^6.11.2",
        "redux": "^4.2.1",
        "rsuite": "^5.34.1",
        "sass": "^1.62.1",
        "sass-loader": "^13.3.1",
        "style-loader": "^3.3.3",
        "terser-webpack-plugin": "^5.3.9",
        "to-string-loader": "^1.2.0",
        "typescript": "^5.0.4",
        "webpack": "^5.84.1",
        "webpack-cli": "^5.1.1",
        "webpack-dev-server": "^4.15.0",
        "webpack-merge": "^5.9.0"
    }
}
